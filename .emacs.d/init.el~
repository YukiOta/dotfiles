;; You may delete these explanatory comments.
(package-initialize)

(package-intialize)

;;Macのとき
(when (qual system-type 'darwin)
  (require 'cask)
      )

;;Linuxの場合
(when (equal system-type 'gnu/linux)
  (require 'cask "~/.cask/cask.el")
  )
(cask-initialize)

;;bind-key
(require 'bind-key)

;;init-loader
(require 'init-loader)
